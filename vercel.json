{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py",
      "methods": ["GET", "POST", "OPTIONS"]
    }
  ],
  "functions": {
    "app.py": {
      "maxDuration": 30,
      "memory": 3008
    }
  },
  "env": {
    "PYTHON_VERSION": "3.9"
  }
}
